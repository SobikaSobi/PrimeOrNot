public class PrimeExample{    
 public static void main(String args[]){    
  int n = 6;
  int count = 0;
  for(int i = 1;i<= n;i++)
   {
       if(n%i==0){
           count++;
       }
   }
   if(count==2){
    System.out.println("The Number is  prime"); 
} else{
      System.out.println("The Number is not prime");
 }
 }
}
